#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <string.h>

typedef struct{
    char* text;
    char* questionText;
    char* answers[4][64];
    int correctAnswer;
}Questions;


int main()
{
    Questions questions[22];
    FILE *file = fopen ("questions.txt", "r" );
    int count = 0;
    char line[1024];
    char* token = NULL;
    if ( file != NULL )
    {
        while( fgets(line,sizeof line, file) != NULL){
            if(count<22){
                token = strtok(line, "#");
                questions[count].questionText = token;
                printf("%s",token);
                for(int i = 0;i <5;++i){
                    token = strtok(NULL, "#");
                    printf("%s\n",token);
                }
                count++;
            }
        }
    }
    fclose(file);
    for(int i = 0;i<22;++i){
        printf("%s\n",questions[i].questionText);
    }
    return 0;
}

